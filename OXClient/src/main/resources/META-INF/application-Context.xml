<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">


	<bean id="gameFrame" class="com.endava.gui.GameFrameImpl">
		<constructor-arg ref="gameController" />
	</bean>

	<bean id="gameController" class="com.endava.controller.GameControllerImpl">
		<constructor-arg ref="gameHandle" />
	</bean>

	<bean id="gameHandle" class="com.endava.model.GameHandleImpl" />

</beans>